#searchIcons{display:grid;grid-template-columns:auto 10%;margin-bottom:1em}#searchIcons__btn{padding:.2em}#icons{display:grid;grid-template-columns:repeat(auto-fit,minmax(128px,3fr));gap:1.2em;list-style-type:none}#icons>li{max-width:12em;position:relative;border:1px solid #a7aeb5;isolation:isolate}#icons>li>svg{width:auto;height:auto}#icons>li>button,#icons>li>dfn,#icons>li>span{position:absolute}#icons>li button{top:0;right:0;padding:0}#icons>li dfn{top:0;left:0;padding:.1em;max-width:25%}#icons>li>span{bottom:5px;left:5px;max-width:95%;overflow:hidden;text-overflow:ellipsis;font-size:.8em;z-index:1}#icons>li:hover{background-color:rgba(255,255,255,.1);cursor:pointer}#icons>li:hover::after{visibility:visible}#icons>li:active{border:1px solid #eaeaea}#icons>li::after{visibility:hidden;content:"";position:absolute;bottom:0;left:0;width:100%;height:35%;background:linear-gradient(to top,black,transparent)}dfn.iconAnimated{background:#0d5910}dfn.iconColored{background:#590f5c}#previewSection{display:grid;height:100%;width:100%;max-height:100%;grid-template-rows:1fr auto}#iconPreview{display:flex;justify-content:center;align-items:center;width:100%;max-height:100%;border-bottom:1px solid #a7aeb5;background-image:url(/images/checkerboard-tiled.png)}#iconPreview svg{max-height:100%;pointer-events:all}#iconPreviewSettings{display:flex;flex-wrap:wrap;justify-content:space-between;margin-top:1em}#iconPreviewSettings input,#iconPreviewSettings label{display:inline;margin:0 .5em}#iconPreviewSettings>div{background-color:rgba(255,255,255,.2);padding:.6em;border-radius:15px}