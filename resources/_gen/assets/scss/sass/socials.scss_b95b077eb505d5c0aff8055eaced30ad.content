@font-face{font-family:minecraft;font-style:normal;font-display:swap;font-weight:400;src:local("Minecraft "),local("Minecraft"),url(/fonts/minecraft.woff2)format("woff2"),url(/fonts/minecraft.woff)format("woff")}@font-face{font-family:minecraftheaderfont;src:url(/fonts/minecraftten.woff)}*{padding:0;margin:0;box-sizing:border-box}html,body{overflow-x:hidden;min-height:100vh;font-family:open sans,Calibri,Tahoma,sans-serif;color:#e8e8e8;background:#1c1c1c}h1,h2{font-family:minecraftheaderfont,sans-serif;font-weight:400;padding:20px 2px}h1{font-size:xx-large}h2{font-size:x-large}h3,h4,h5,h6{font-family:minecraft,gill sans,Calibri,Tahoma,sans-serif;font-weight:400;padding:20px 0}a{text-decoration:none;color:#fff}a:active{text-decoration:underline}button{background:0 0;appearance:none;border:none;cursor:pointer}#intro{text-align:center}#intro h3{font-style:italic;font-size:small}#intro h1,#intro h2,#intro h3{display:inline}#intro h6{position:absolute;top:0;left:1em}hr{display:block;height:1px;width:60%;border:0;border-top:1px solid rgba(204,204,204,.734);margin:1em auto;padding:0}details{background:#595959;border-radius:5px;box-shadow:0 4px 10px #0000009f;padding:10px;margin:10px 0;width:minmax(60%,100%)}details[open]>summary{padding-bottom:8px}summary{cursor:pointer;border:none;font-weight:700}article{display:grid;grid-template-areas:"socials ingame contact";grid-template-columns:2fr 2fr 1fr;gap:20px;margin:20px}article section{background:#2e2e2e;padding:20px;border-radius:10px;border:1px solid #cecece;box-shadow:0 4px 10px #ffffffba;transition:box-shadow,transform,1s ease}article section:hover{border:1px solid #fff;box-shadow:0 6px 10px #ffffff4b;transform:translateY(-2%);transition:box-shadow,transform,border,1s ease}article h3{padding:0;padding-bottom:10px;text-align:center}.link{background:#b5b5b5;padding:10px;border-radius:5px;border:2px solid #fff;text-align:center;transition:background 1s;width:auto;margin:10px 0;position:relative}.link span,.link i{color:#000}.link:hover{background:#fff}.link:active{box-shadow:0 0 10px #ffffffba}.infoBtn{position:absolute;right:5px;top:50%;transform:translateY(-50%);padding:5px 6px 4.5px;font-size:1.2rem;width:auto;border-radius:5px;color:#1c1c1c}.infoBtn:hover{background:rgba(130,130,130,.565)}.infoBtn i{color:#313131;pointer-events:none}#socials{grid-area:socials}#ingame{grid-area:ingame;display:flex;flex-direction:column;align-items:center}#ingame select{font-family:minecraft,gill sans,Calibri,Tahoma,sans-serif;margin-bottom:15px;width:auto}#skin_div[data-animation]{animation:rainbowshadow 2s linear;border-radius:5px}@keyframes rainbowshadow{100%,0%{border:5px solid red}8%{border:5px solid #ff7f00}16%{border:5px solid #ff0}25%{border:5px solid #7fff00}33%{border:5px solid lime}41%{border:5px solid #00ff7f}50%{border:5px solid cyan}58%{border:5px solid #007fff}66%{border:5px solid blue}75%{border:5px solid #7f00ff}83%{border:5px solid #f0f}91%{border:5px solid #ff007f}}#contact{grid-area:contact}.icons i{transition:color,.2s ease}.icons.twitter:hover i:not(.noIcons){color:#1da1f2}.icons.youtube:hover i:not(.noIcons){color:red}.icons.discord:hover i:not(.noIcons){color:#7289da}.icons.reddit:hover i:not(.noIcons){color:#ff4500}.icons.github:hover i:not(.noIcons){color:#c9c9c9}.fa-star:hover{animation:rainbow 5s linear;animation-iteration-count:infinite}@keyframes rainbow{100%,0%{color:red}8%{color:#ff7f00}16%{color:#ff0}25%{color:#7fff00}33%{color:lime}41%{color:#00ff7f}50%{color:cyan}58%{color:#007fff}66%{color:blue}75%{color:#7f00ff}83%{color:#f0f}91%{color:#ff007f}}.mcbutton{background-color:#393939;padding:5px;border-bottom:2px solid #1d1d1d;border-right:2px solid #1d1d1d;border-left:2px solid #474747;border-top:2px solid #474747;outline:2px solid #000;color:#aaa}.mcbutton:hover{background-color:#595959;border-bottom:2px solid #2f2f2f;border-right:2px solid #2f2f2f;border-left:2px solid #6d6d6d;border-top:2px solid #6d6d6d;outline:2px solid #fff;color:#fff}.mcbutton:active{background-color:var(--btn-hover-bg);border-top:2px solid #2f2f2f;border-left:2px solid #2f2f2f;border-right:2px solid #6d6d6d;border-bottom:2px solid #6d6d6d;outline:2px solid #fff;color:#fff}.hidden{animation:fadeOut .5s ease forwards!important}@keyframes fadeIn{0%{opacity:0}100%{opacity:1}}@keyframes fadeOut{0%{opacity:1}100%{opacity:0}}#detailsPopup{animation:fadeIn .5s ease forwards;display:none;position:fixed;max-width:50%;width:auto;height:auto;left:50%;top:50%;transform:translate(-50%,-50%);flex-direction:column;align-items:flex-start;font-size:1.4rem;backdrop-filter:blur(8px);padding:20px;border-radius:10px;border:5px solid #ffffff3c;box-shadow:0 0 20px 20px #0000009f}#detailsPopup__header{flex:1;display:flex;align-items:center}#detailsPopup__header img{height:65px;width:auto;border-radius:10px}#detailsPopup__header h3{margin-left:15px;font-size:1.8em;padding:0}#detailsPopup__desc{flex:2}#detailsPopup>a{flex:2;border-radius:50px;padding:15px;text-align:center;font-size:1.1em;position:relative}#detailsPopup>a span{font-weight:700}#detailsPopup>a i{margin-left:5px}#detailsPopup>a::before{content:'';transition:opacity .7s ease;background:linear-gradient(90deg,#57ebde 0%,#aefb2a 100%);width:100%;height:100%;position:absolute;top:0;left:0;opacity:.7;z-index:-1;border-radius:50px}#detailsPopup>a:hover::before{opacity:.9}#detailsPopup>a:active::before{outline:3px solid #fff}#detailsPopup>*{margin:10px}@media screen and (max-width:992px){article{grid-template-areas:"socials ingame" "contact contact";grid-template-columns:1fr 1fr}#detailsPopup{max-width:75%;font-size:1.2rem}}@media screen and (max-width:725px){article{grid-template-areas:"socials" "ingame" "contact";grid-template-columns:1fr}#intro h6{position:inherit}#detailsPopup{max-width:96%;min-width:60%;font-size:1.1rem}#detailsPopup__header{flex-wrap:wrap}#detailsPopup__header img{height:50px}#detailsPopup__header h3{margin-left:5px;font-size:1.1em}#detailsPopup>*{margin:5px}#detailsPopup__desc{min-width:20ch}#detailsPopup>a{border-radius:20px;font-size:.9em}}