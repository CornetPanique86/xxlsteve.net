@font-face{font-family:minecraft;font-style:normal;font-display:swap;font-weight:400;src:local("Minecraft "),local("Minecraft"),url(/fonts/minecraft.woff2)format("woff2"),url(/fonts/minecraft.woff)format("woff")}@font-face{font-family:minecraftheaderfont;src:url(/fonts/minecraftten.woff)}*{padding:0;margin:0;box-sizing:border-box}html,body{overflow-x:hidden;min-height:100vh;font-family:Calibri,Tahoma,sans-serif;color:#e8e8e8;background:#1c1c1c}h1,h2{font-family:minecraftheaderfont,sans-serif;font-weight:400;padding:20px 0}h1{font-size:xx-large}h2{font-size:x-large}h3,h4,h5,h6{font-family:minecraft,gill sans,Calibri,Tahoma,sans-serif;font-weight:400;padding:20px 0}a{text-decoration:none;color:#fff}a:active{text-decoration:underline}#intro{text-align:center}#intro h3{font-style:italic;font-size:small}#intro h1,#intro h2,#intro h3{display:inline}#intro h6{position:absolute;top:0;left:1em}hr{display:block;height:1px;width:60%;border:0;border-top:1px solid rgba(204,204,204,.734);margin:1em auto;padding:0}details{background:#595959;border-radius:5px;box-shadow:0 4px 10px #0000009f;padding:10px;margin:10px 0;width:minmax(60%,100%)}details[open]>summary{padding-bottom:8px}summary{cursor:pointer;border:none;font-weight:700}article{display:grid;grid-template-areas:"socials ingame contact";gap:20px;margin:20px}article section{background:#2e2e2e;padding:20px;border-radius:10px;border:1px solid #cecece;box-shadow:0 4px 10px #ffffffba;transition:box-shadow,transform,1s ease}article section:hover{border:1px solid #fff;box-shadow:0 6px 10px #ffffff4b;transform:translateY(-2%);transition:box-shadow,transform,border,1s ease}article h3{padding:0;padding-bottom:10px;text-align:center}.link{background:#b5b5b5;padding:10px;border-radius:5px;border:2px solid #fff;text-align:center;transition:background 1s;width:auto;margin:10px 0}.link span,.link i{color:#000}.link:hover{background:#fff}.link:active{box-shadow:0 0 10px #ffffffba}#socials{grid-area:socials}#ingame{grid-area:ingame;display:flex;flex-direction:column;align-items:center}#ingame select{font-family:minecraft,gill sans,Calibri,Tahoma,sans-serif;margin-bottom:15px;width:auto}#contact{grid-area:contact}@media screen and (max-width:992px){article{grid-template-areas:"socials ingame" "contact contact"}}@media screen and (max-width:600px){article{grid-template-areas:"socials" "ingame" "contact"}}.icons i{transition:color,.2s ease}.icons.twitter:hover i{color:#1da1f2}.icons.youtube:hover i{color:red}.icons.discord:hover i{color:#7289da}.icons.reddit:hover i{color:#ff4500}.icons.github:hover i{color:#c9c9c9}.mcbutton{background-color:#393939;padding:5px;border-bottom:2px solid #1d1d1d;border-right:2px solid #1d1d1d;border-left:2px solid #474747;border-top:2px solid #474747;outline:2px solid #000;color:#aaa}.mcbutton:hover{background-color:#595959;border-bottom:2px solid #2f2f2f;border-right:2px solid #2f2f2f;border-left:2px solid #6d6d6d;border-top:2px solid #6d6d6d;outline:2px solid #fff;color:#fff}.mcbutton:active{background-color:var(--btn-hover-bg);border-top:2px solid #2f2f2f;border-left:2px solid #2f2f2f;border-right:2px solid #6d6d6d;border-bottom:2px solid #6d6d6d;outline:2px solid #fff;color:#fff}